	  <None Include="wwwroot\assets\shaders\OpenGL\Quad.glsl" />
	  <None Include="wwwroot\assets\textures\Checkerboard.png" />
	  <None Include="wwwroot\assets\textures\ChernoLogo.png" />
	  <None Include="wwwroot\resources\icons\contentbrowser\DirectoryIcon.png" />
	  <None Include="wwwroot\resources\icons\contentbrowser\FileIcon.png" />
	  <None Include="wwwroot\resources\icons\PauseButton.png" />
	  <None Include="wwwroot\resources\icons\PlayButton.png" />
	  <None Include="wwwroot\resources\icons\SimulateButton.png" />
	  <None Include="wwwroot\resources\icons\StepButton.png" />
	  <None Include="wwwroot\resources\icons\StopButton.png" />
	  <None Include="wwwroot\resources\Scripts\Lucky-ScriptCore.dll" />
	  <None Include="wwwroot\resources\Scripts\Lucky-ScriptCore.pdb" />
	</ItemGroup>

	<PropertyGroup>
		<EmccExtraLDFlags>-g -sUSE_WEBGL2=1 -sFULL_ES3=1 -sUSE_GLFW=3 -sWASM=3 -sMIN_WEBGL_VERSION=2 -sMEMORY64=0 -sFORCE_FILESYSTEM=1 -sALLOW_MEMORY_GROWTH=1 -sNO_EXIT_RUNTIME=0 --use-preload-plugins --no-heap-copy -lidbfs.js</EmccExtraLDFlags>
		<WasmEmitSymbolMap>true</WasmEmitSymbolMap>
		<WasmAllowUndefinedSymbols>true</WasmAllowUndefinedSymbols>
		<OutputType>Exe</OutputType>
		<RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
		<AllowUnsafeBlocks>True</AllowUnsafeBlocks>
		<BaseOutputPath>..\..\bin\browser</BaseOutputPath>
	</PropertyGroup>

</Project>
